prueba2015 <- data.frame(Country = datos2015$Country), HS2015 = datos2015$HS)
prueba2015 <- data.frame(Country = datos2015$Country, HS2015 = datos2015$HS)
View(prueba2015)
prueba2016 <- data.frame(Country = datos2016$Country, HS2016 = datos2016$HS)
View(prueba2016)
prueba2017 <- data.frame(Country = datos2017$Country, HS2017 = datos2017$HS)
View(prueba2017)
margen_1 <-merge(x=prueba2015,y=prueba2016,by="Country",all = TRUE)
View(margen_1)
margen_2 <-merge(x=margen_1,y=prueba2017,by="Country",all = TRUE)
View(margen_2)
head(margen_2,20)
#Transformar anteriores y mostrar informacion
pruebas2_2015 <- data.frame(Country = datos2015$Country,HS = datos2015$HS,Group='2015')
View(pruebas2_2015)
pruebas2_2016 <- data.frame(Country = datos2016$Country,HS = datos2016$HS,Group='2016')
View(pruebas2_2016)
pruebas2_2017 <- data.frame(Country = datos2017$Country,HS = datos2017$HS,Group='2017')
View(pruebas2_2017)
margen_3 <-merge(x=pruebas2_2015,y=pruebas2_2016,by="Country",all = TRUE)
View(margen_3)
margen_3 <-merge(x=pruebas2_2015,y=pruebas2_2016,all = TRUE)
margen_4 <-merge(x=margen_3,y=pruebas2_2017,all = TRUE)
View(margen_4)
grupos2015 <- subset(margen_4$HS,margen_4$Group=='2015')
View(grupos2015)
grupos2016 <- subset(margen_4$HS,margen_4$Group=='2016')
grupos2017 <- subset(margen_4$HS,margen_4$Group=='2017')
boxplot(margen_4$HS ~ margen_4$Group,main="Distribución del nivel de felicidad en tres años",names=c("2015","2016","2017"))
