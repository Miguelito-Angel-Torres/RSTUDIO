datos2015 <- read.csv("datos/happy2015.csv",sep=",", na.strings = "NA", dec=".")
view(datos2015)
View(datos2015)
datos2016 <- read.csv("datos/happy2016.csv",sep=",", na.strings = "NA", dec=".")
datos2017 <- read.csv("datos/happy2017.csv",sep=",", na.strings = "NA", dec=".")
View(datos2016)
View(datos2015)
View(datos2017)
# Reducir el nombre de las columnas
